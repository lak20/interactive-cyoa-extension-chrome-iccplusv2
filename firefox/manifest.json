{
  "manifest_version": 3,
  "name": "Interactive CYOA assist tool for icc+v2",
  "version": "0.0.3.7",
  "browser_specific_settings": {
    "gecko": {
      "id": "interactiveassisttoolsvelte@cyoa",
      "update_url": "https://raw.githubusercontent.com/lak20/interactive-cyoa-extension-chrome-iccplusv2/master/firefox/updates.json"
    }
  },
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png"
    },
    "default_popup": "action/action.html"
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content/content-script.js"
      ],
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "background/service-worker.js"
    ]
  },
  "externally_connectable": {
    "ids": [
      "*"
    ],
    "matches": [
      "*"
    ]
  },
  "description": "A tool to assist filling out interactive CYOAs made with the Interactive CYOA creator",
  "author": "lak20",
  "short_name": "CYOA tool svelte"
}
